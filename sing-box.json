{
  "log": {
    "level": "info",
    "timestamp": true
  },

  "dns": {
    "independent_cache": true,
    "servers": [
      {
        "tag": "dns_cn",
        "type": "https",
        "server": "223.5.5.5"
      },
      {
        "tag": "dns_proxy",
        "type": "https",
        "server": "8.8.8.8",
        "detour": "proxy"
      },
      {
        "tag": "dns_fakeip",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "rule_set": "cn_domain",
        "server": "dns_cn"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "dns_fakeip",
        "rewrite_ttl": 1
      }
    ],
    "final": "dns_proxy"
  },

  "route": {
    "auto_detect_interface": true,
    "default_domain_resolver": {
      "server": "dns_cn"
    },

    "rules": [
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },

      {
        "ip_is_private": true,
        "outbound": "direct"
      },

      {
        "rule_set": [
          "cn_ip",
          "cn_domain"
        ],
        "outbound": "cn"
      },

      {
        "rule_set": [
          "google_ip",
          "google_domain"
        ],
        "outbound": "google"
      },

      {
        "rule_set": [
          "apple_ip",
          "apple_domain"
        ],
        "outbound": "apple"
      },

      {
        "rule_set": [
          "telegram_ip",
          "telegram_domain"
        ],
        "outbound": "telegram"
      },

      {
        "rule_set": [
          "netflix_ip",
          "netflix_domain"
        ],
        "outbound": "netflix"
      },

      {
        "rule_set": "youtube_domain",
        "outbound": "youtube"
      },

      {
        "rule_set": "bilibili_domain",
        "outbound": "bilibili"
      },

      {
        "rule_set": "openai_domain",
        "outbound": "openai"
      },

      {
        "rule_set": "proxy_domain",
        "outbound": "proxy"
      }
    ],

    "final": "final",

    "rule_set": [
      {
        "tag": "cn_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geoip/cn.srs",
        "download_detour": "direct"
      },
      {
        "tag": "cn_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo-lite/geosite/cn.srs",
        "download_detour": "direct"
      },

      {
        "tag": "proxy_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geosite/geolocation-!cn.srs",
        "download_detour": "direct"
      },

      {
        "tag": "google_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo-lite/geoip/google.srs"
      },
      {
        "tag": "google_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo-lite/geosite/google.srs"
      },

      {
        "tag": "apple_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo-lite/geoip/apple.srs"
      },
      {
        "tag": "apple_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo-lite/geosite/apple.srs"
      },

      {
        "tag": "telegram_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo-lite/geoip/telegram.srs"
      },
      {
        "tag": "telegram_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo-lite/geosite/telegram.srs"
      },

      {
        "tag": "netflix_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo-lite/geoip/netflix.srs"
      },
      {
        "tag": "netflix_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo-lite/geosite/netflix.srs"
      },

      {
        "tag": "youtube_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo-lite/geosite/youtube.srs"
      },

      {
        "tag": "bilibili_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo-lite/geosite/bilibili.srs"
      },

      {
        "tag": "openai_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geosite/openai.srs"
      }
    ]
  },

  "inbounds": [
    {
      "type": "tun",
      "stack": "mixed",
      "mtu": 9000,
      "address": [
        "172.19.0.0/30",
        "fdfe:dcba:9876::0/126"
      ],
      "auto_route": true,
      "sniff": true
    },
    {
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 7890
    }
  ],

  "outbounds": [
    { "tag": "direct", "type": "direct" },

    {
      "tag": "proxy",
      "type": "selector",
      "outbounds": [
        "all-auto",
        "direct"
      ],
      "default": "all-auto"
    },

    {
      "tag": "all-auto",
      "type": "urltest",
      "outbounds": [],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "2m",
      "tolerance": 80
    },

    { "tag": "google", "type": "selector", "outbounds": ["proxy", "direct"] },
    { "tag": "apple", "type": "selector", "outbounds": ["direct", "proxy"] },
    { "tag": "telegram", "type": "selector", "outbounds": ["proxy"] },
    { "tag": "netflix", "type": "selector", "outbounds": ["proxy"] },
    { "tag": "youtube", "type": "selector", "outbounds": ["proxy"] },
    { "tag": "bilibili", "type": "selector", "outbounds": ["direct", "proxy"] },
    { "tag": "openai", "type": "selector", "outbounds": ["proxy"] },
    { "tag": "cn", "type": "selector", "outbounds": ["direct"] },
    { "tag": "final", "type": "selector", "outbounds": ["proxy", "direct"] }
  ],

  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui"
    }
  }
}
